package v;
import java.util.Scanner;

public class Table {
	public static void main(String[] arg) {
		Scanner scanner=new Scanner(System.in);
//		int i=2;
//	int n = 2;
		System.out.println("Enter the number :  ");
		
		int n =scanner.nextInt();
		
		for(int i=1;i<=10;i++)
			System.out.println(n+"*"+i+"="+n*i);
	}

}
