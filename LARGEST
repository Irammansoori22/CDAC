package v;

import java.util.Scanner;

public class Maxmin {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
//		int a =s.nextInt();
		int largest=0;
		
	int[] arr = {10,89,79};
		for(int i=0;i<arr.length;i++)
		{
			if(arr[i]>largest) 
			{
			largest=arr[i];
			
//			System.out.println("Print the largest number: "+Smallest);
		}
		}
		System.out.println("Print the largest number: "+largest);
		}

}
